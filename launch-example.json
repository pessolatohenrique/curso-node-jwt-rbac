{
  /*
    Example to debug into VS CODE
    */
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}",
      "skipFiles": ["<node_internals>/**"],
      "restart": true,
      "protocol": "inspector"
    }
  ]
}
